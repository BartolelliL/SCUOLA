#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;
    for (int i = 0; i < N; i++) cout << i * 2 << " ";
    cout << endl;
    for (int i = 0; i < N; i++) cout << i * 2 + 1 << " ";
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int a, b;
    cin >> a >> b;
    for (int i = a; i <= b; i++)
        if (i % 2 == 0) cout << i << " ";
    return 0;
}



#include <iostream>
using namespace std;

int main() {
    int limite, x, cont = 0;
    cin >> limite;
    while (cont < limite) {
        cin >> x;
        cont++;
        if (x % 7 == 0) {
            cout << cont;
            return 0;
        }
    }
    cout << "fallimento";
    return 0;
}

#include <iostream>
using namespace std;

int main() {
    int X, Y, somma = 0;
    cin >> X >> Y;
    for (int i = 1; i <= X; i++) somma += i * i;
    cout << somma;
    return 0;
}